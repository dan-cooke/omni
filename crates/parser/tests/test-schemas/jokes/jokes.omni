// This is the most basic parser test
// We are defining a simple jokes-service that
// will return a joke

service JokeService {
	version: "1.0.0",
	resources: [Joke]
}

@resource(operations: [GetJoke])
struct Joke {
	body: String,
	author: Author,
}

@resource(operations: [GetAuthor])
struct Author {
	name: String,
	address: Address
}

struct Address {
	postcode: String,
}


operation GetJoke {
	type: "read",
	output: Joke

}
